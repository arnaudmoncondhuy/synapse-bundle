{
    "name": "arnaudmoncondhuy/synapse-bundle",
    "type": "symfony-bundle",
    "description": "A reusable AI Chatbot bundle for Symfony powered by Google Gemini.",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Moncondhuy",
            "email": "arnaud@makerlab.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0 || ^8.0",
        "symfony/asset": "^7.0 || ^8.0",
        "symfony/twig-bundle": "^7.0 || ^8.0",
        "symfony/stimulus-bundle": "^2.0",
        "symfony/security-bundle": "^7.0 || ^8.0",
        "doctrine/orm": "^2.19 || ^3.0",
        "symfony/yaml": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ArnaudMoncondhuy\\SynapseBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArnaudMoncondhuy\\SynapseBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "require-dev": {
        "symfony/test-pack": "^1.2",
        "symfony/process": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.93"
    }
}