{
    "name": "@arnaudmoncondhuy/synapse-bundle",
    "description": "AI Chatbot integration for Symfony powered by Google Gemini",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "symfony-ux",
        "ai",
        "chatbot",
        "gemini",
        "stimulus"
    ],
    "homepage": "https://github.com/arnaudmoncondhuy/synapse-bundle",
    "repository": "https://github.com/arnaudmoncondhuy/synapse-bundle",
    "type": "module",
    "main": "assets/controllers/synapse_chat_controller.js",
    "symfony": {
        "controllers": {
            "synapse-chat": {
                "main": "assets/controllers/synapse_chat_controller.js",
                "fetch": "eager",
                "enabled": true
            },
            "synapse-sidebar": {
                "main": "assets/controllers/synapse_sidebar_controller.js",
                "fetch": "eager",
                "enabled": true
            }
        }
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.0.0"
    }
}